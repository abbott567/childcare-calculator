{
  "name": "childcare-tests",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "xo"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "date-fns": "^2.0.0-alpha.22",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "nunjucks": "^3.1.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "xo": "^0.23.0"
  },
  "xo": {
    "space": true,
    "esnext": true,
    "env": [
      "jquery",
      "browser",
      "mocha"
    ],
    "rules": {
      "xo/no-process-exit": 0,
      "import/no-unassigned-import": 0,
      "unicorn/filename-case": 0,
      "max-nested-callbacks": 0,
      "no-unused-vars": [
        2,
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
